super + w
  connman-gtk
super + s
  flameshot gui

super + shift + d
  lobster --dmenu

super + shift + p
  loginctl kill-session $XDG_SESSION_ID

XF86AudioPlay
  playerctl play-pause

# reload config
super + shift + x
	pkill -USR1 -x sxhkd

# task manager

control + shift + mode_switch
	alacritty -e btop
